<%= form_tag sign_in_path do %>
<p>
  <%= label_tag "Email: " %><br />
  <%= text_field_tag :email %>
</p>
<p>
  <%= label_tag "Password: " %><br />
  <%= password_field_tag :password %>
</p>
<p class="button"><%= submit_tag "Sign In" %></p>
<% end %>


<%= link_to "Sign out", sign_out_path, class: "button", method: "delete" %>
